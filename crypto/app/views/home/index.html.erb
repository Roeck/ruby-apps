<div class="jumbotron">
 <center>
	<h1>Crypto Currency</h1>
	  <h3>Ranking:</h3>
        <br/><br/>

		<strong>
           <% @coins %>
            <% for x in @coins %>
			    <% for coin in @my_coins %>
			        <% if coin == x["symbol"] %>
			            <%= x["name"] %>: $<%= x["price_usd"] %><br/>
			            Rank: <%= x["rank"] %><br/><br/>
			        <% end %>
			    <% end %>
			<% end %>
        </strong>

	<br/>
 </center>
</div>



        



